Sub TableRowBreakOff()
' Attribute TableRowBreakOff.VB_Description = "Macro recorded 2/06/2009 by Warren Lewington"
' Attribute TableRowBreakOff.VB_ProcData.VB_Invoke_Func = "Project.NewMacros.TableRowRepeatOff"
'
' TableRowBreakOff Macro
' Macro recorded 2/06/2009 by Warren Lewington
'
    Selection.Tables(1).Rows.AllowBreakAcrossPages = False
End Sub
